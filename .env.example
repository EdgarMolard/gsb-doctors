# Configuration Docker pour GSB-DOCTORS
# Copiez ce fichier vers .env et modifiez les valeurs selon vos besoins

# Base de données
DB_NAME=gsbrapports
DB_USER=gsb_user
DB_PASSWORD=gsb_password
DB_ROOT_PASSWORD=gsb_root_2026
DB_PORT=3306

# API Backend
API_PORT=3000

# Frontend
FRONTEND_PORT=4200

# CORS (Cross-Origin Resource Sharing)
CORS_ORIGIN=http://localhost:4200

# PHP Configuration (pour développement)
PHP_DISPLAY_ERRORS=0
PHP_ERROR_REPORTING=E_ERROR

# Environnement
NODE_ENV=production
